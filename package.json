{
  "name": "nest-pass-emploi-api",
  "version": "2.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "npm": "8.19.2",
    "yarn": "3.2.3",
    "node": "16.15.0"
  },
  "bin": {
    "vault": "./.vault/vault.cjs"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:fix": "eslint --ext \".ts\" --fix --ignore-path .gitignore .",
    "prepare": "husky install",
    "migration": "sequelize db:migrate",
    "psql": "docker-compose exec apidb psql -h localhost -d passemploidb -U passemploi ",
    "seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "seed:referentiel": "sequelize db:seed:undo --seed 20211201160540-referentiel-communes-et-departements.js && sequelize db:seed --seed 20211201160540-referentiel-communes-et-departements.js && sequelize db:seed:undo --seed 20220420160540-referentiel-agences.js && sequelize db:seed --seed 20220420160540-referentiel-agences.js",
    "start": "yarn start:pg:db && nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prod:worker": "IS_WEB=false IS_WORKER=true APM_IS_ACTIVE=false node dist/main",
    "start:pg:db": "docker-compose up -d apidb && ./src/infrastructure/sequelize/wait-for-db.sh && yarn migration",
    "start:redis:db": "docker-compose up -d redisdb",
    "start:db:test": "yarn start:db:pg:test && yarn start:db:redis:test",
    "start:db:pg:test": "docker-compose up -d pgtestdb && ./test/utils/wait-for-db.sh && DATABASE_URL=postgresql://test:test@localhost:56432/test yarn migration",
    "start:db:redis:test": "docker-compose up -d redistestdb",
    "test:local:unit": "ATABASE_URL=postgresql://test:test@localhost:56432/test mocha 'test/**/*.test.ts' --ignore 'test/**/*.db.test.ts' --parallel --exit --timeout 10000",
    "test:local:db": "yarn start:db:test && DATABASE_URL=postgresql://test:test@localhost:56432/test mocha 'test/**/*.db.test.ts' --exit --timeout 10000",
    "test:ci:db": "yarn migration && mocha 'test/**/*.db.test.ts' --exit --timeout 10000",
    "test": "yarn cover:unit && yarn cover:db && npm run cover:report",
    "cover:unit": "nyc yarn test:local:unit",
    "cover:db": "nyc --no-clean yarn test:local:db",
    "cover:ci:db": "nyc --no-clean yarn test:ci:db",
    "test:ci": "yarn migration && nyc --reporter=lcovonly mocha 'test/**/*.test.ts' --exit --timeout 10000",
    "cover:report": "nyc report --reporter=lcovonly",
    "watch": "yarn start:pg:db && yarn start:redis:db && nest start --watch | npx pino-pretty",
    "watch:worker": "yarn start:pg:db && yarn start:redis:db && IS_WORKER=true nest start --watch | npx pino-pretty",
    "run:with:apm": "yarn start:pg:db && yarn start:redis:db && dotenv -e .environment nest start --watch | npx pino-pretty",
    "tasks:init-all-jobs": "IS_WEB=false IS_WORKER=false TASK_NAME=INIT_ALL_JOBS node dist/main",
    "tasks:dummy-job": "IS_WEB=false IS_WORKER=false TASK_NAME=DUMMY_JOB node dist/main",
    "tasks:nettoyer-les-job": "IS_WEB=false IS_WORKER=false TASK_NAME=NETTOYER_LES_JOBS node dist/main",
    "tasks:rechercher-les-nouvelles-offres-poles-emploi": "IS_WEB=false IS_WORKER=false TASK_NAME=NOUVELLES_OFFRES_EMPLOI node dist/main",
    "tasks:rechercher-les-nouvelles-offres-civiques": "IS_WEB=false IS_WORKER=false TASK_NAME=NOUVELLES_OFFRES_SERVICE_CIVIQUE node dist/main",
    "tasks:envoyer-mail-conseiller-messages": "IS_WEB=false IS_WORKER=false TASK_NAME=MAIL_CONSEILLER_MESSAGES node dist/main",
    "tasks:initialiser-les-crons": "IS_WEB=false IS_WORKER=false TASK_NAME=INITIALISER_LES_CRON node dist/main",
    "tasks:mettre-a-jour-mailing-list-conseiller": "IS_WEB=false IS_WORKER=false TASK_NAME=UPDATE_CONTACTS_CONSEILLER_MAILING_LISTS node dist/main",
    "tasks:recuperer-les-situations-milo": "IS_WEB=false IS_WORKER=false TASK_NAME=RECUPERER_SITUATIONS_JEUNES_MILO node dist/main",
    "tasks:nettoyer-les-pieces-jointes": "IS_WEB=false IS_WORKER=false TASK_NAME=NETTOYER_LES_PIECES_JOINTES node dist/main",
    "tasks:nettoyer-les-donnees": "IS_WEB=false IS_WORKER=false TASK_NAME=NETTOYER_LES_DONNEES node dist/main",
    "tasks:notifier-rendez-vous-pe": "IS_WEB=false IS_WORKER=false TASK_NAME=NOTIFIER_RENDEZVOUS_PE node dist/main",
    "dump-restore-db": "scripts/db_dump_restore.sh",
    "dump-restore-db:local": "dotenv -e .environment yarn dump-restore-db",
    "release:patch": "scripts/release.sh patch",
    "release:minor": "scripts/release.sh minor",
    "release:major": "scripts/release.sh major"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.163.0",
    "@aws-sdk/s3-request-presigner": "^3.163.0",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.1.2",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.2",
    "@nestjs/platform-express": "9.1.2",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/terminus": "9.1.1",
    "bull": "4.9.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "^1.7.4",
    "crypto-js": "^4.1.1",
    "elastic-apm-node": "3.38.0",
    "firebase-admin": "11.0.1",
    "geojson": "^0.5.0",
    "helmet": "^6.0.0",
    "ics": "^2.40.0",
    "joi": "^17.6.0",
    "jose": "^4.9.2",
    "luxon": "3.0.3",
    "nestjs-pino": "3.1.1",
    "openid-client": "5.1.9",
    "pg": "8.8.0",
    "pg-connection-string": "2.5.0",
    "pino-http": "8.2.0",
    "remove-accents": "0.4.3",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "sequelize": "6.23.1",
    "sequelize-cli": "6.4.1",
    "sequelize-typescript": "2.1.3",
    "swagger-ui-express": "4.5.0",
    "tokenbucket": "^0.3.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.2",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@salesforce/ts-sinon": "^1.3.21",
    "@types/bull": "^3.15.9",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/compression": "^1.7.2",
    "@types/crypto-js": "4.1.1",
    "@types/dirty-chai": "^2.0.2",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.10",
    "@types/luxon": "^3.0.1",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.15",
    "@types/pino-multi-stream": "^5.1.3",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "bull-repl": "^0.28.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.1.0",
    "dirty-chai": "^2.0.1",
    "dotenv-cli": "^6.0.0",
    "dotvault": "^0.0.9",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "redis": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-loader": "^9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2",
    "yargs": "^17.5.1"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "lint-staged": {
    "*.ts": "yarn lint:fix"
  },
  "packageManager": "yarn@3.2.3"
}
